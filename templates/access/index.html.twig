{% extends 'base.html.twig' %}

{% block title %}Access to Location{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>Access to Location</h1>
    <p>Address : {{ accessInfo.address }}</p>
    <p>City : {{ accessInfo.city }}</p>
    <p>Zip Code : {{ accessInfo.zipCode }}</p>
    <p>Directions : {{ accessInfo.directions }}</p>
</div>
{% endblock %}
